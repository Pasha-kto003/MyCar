                @if (User.Identity.IsAuthenticated)
                {                               
                    <a class="dropdown-item" onmouseover="this.style.backgroundColor='Gray'" onmouseout="this.style.backgroundColor='Transparent'" style="color: white; font-size:16px;" asp-controller="Account" asp-action="Logout">Выход (@User.Identity.Name)</a>
                }
                else
                {                               
                    <a class="dropdown-item" onmouseover="this.style.backgroundColor='Gray'" onmouseout="this.style.backgroundColor='#00ffffff'" style="color: white; font-size:16px;" asp-controller="Account" asp-action="Login">Вход</a>                                
